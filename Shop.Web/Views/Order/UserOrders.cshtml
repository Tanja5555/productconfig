@using TanLeonaShop.Domain.Models;
@model  IQueryable<Order> 

<div class="frame">

    @foreach (var item in Model)
    {
         <div class="row">
            <div class="col-md-12">
                Order Code: @Html.DisplayFor(x => item.OrderCode) <br />
                Total Price: @Html.DisplayFor(x => item.TotalPrice) <span>Kr</span><br />
                Order Date: @Html.DisplayFor(x => item.OrderDate)<br />
                Delivery Date: @Html.DisplayFor(x => item.DeliveryDate)<br />
                @Html.Partial("_DeliveredInfo", item)
            </div>
        </div>
        <br />
    }
</div>

